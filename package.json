{
  "name": "boilerplatereact",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "prettier:write": "prettier --config .prettierrc --write .",
    "prettier:check": "prettier --config .prettierrc --check .",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "analyze": "react-app-rewired build && source-map-explorer 'build/static/js/*.js' --html analyze-result.html",
    "audit": "npm audit --production",
    "commit": "git commit"
  },
  "lint-staged": {
    "/*.less": "stylelint --syntax less",
    "/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/inter": "^5.0.16",
    "@fontsource/poppins": "^5.0.8",
    "@fontsource/roboto": "^5.0.8",
    "@material-tailwind/react": "^2.1.9",
    "@mui/icons-material": "^5.16.6",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.16.6",
    "@mui/system": "^5.15.9",
    "@mui/utils": "^5.15.11",
    "@mui/x-date-pickers": "^7.12.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@tabler/icons-react": "^2.47.0",
    "@vitejs/plugin-react": "^4.2.1",
    "apexcharts": "^3.46.2",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.3",
    "boilerplatereact": ".",
    "cssnano": "^7.0.4",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.6",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "material-ui-popup-state": "^5.0.10",
    "notistack": "^3.0.1",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-apexcharts": "^1.4.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-notifications-component": "^4.0.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^9.1.2",
    "react-router": "6.21.3",
    "react-router-dom": "^6.26.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "swr": "^2.2.5",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-svgr": "^4.2.0",
    "web-vitals": "^3.5.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cz-git": "^1.9.4",
    "czg": "^1.9.4",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.4"
  }
}
